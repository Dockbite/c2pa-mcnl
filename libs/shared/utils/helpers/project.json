{
  "name": "shared-utils-helpers",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/utils/helpers/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:util"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/utils/helpers",
        "main": "libs/shared/utils/helpers/src/index.ts",
        "tsConfig": "libs/shared/utils/helpers/tsconfig.lib.json",
        "assets": ["libs/shared/utils/helpers/*.md"],
        "format": ["cjs"]
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../../../coverage/libs/shared/utils/helpers"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
