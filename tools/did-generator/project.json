{
  "name": "did-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/did-generator/src",
  "projectType": "library",
  "tags": ["type:tool", "runtime:node"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/tools/did-generator",
        "main": "tools/did-generator/src/index.ts",
        "tsConfig": "tools/did-generator/tsconfig.lib.json",
        "outputFileName": "index.cjs",
        "platform": "node",
        "format": ["cjs"],
        "bundle": true,
        "assets": []
      }
    },
    "help": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/did-generator/index.cjs --help"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/did-generator/index.cjs generate"
      }
    },
    "keys": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/did-generator/index.cjs keys"
      }
    },
    "did": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/did-generator/index.cjs did"
      }
    },
    "credential": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/did-generator/index.cjs credential"
      }
    }
  }
}
