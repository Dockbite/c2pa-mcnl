{
  "name": "cert-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/cert-generator/src",
  "projectType": "library",
  "tags": ["type:tool", "runtime:node"],
  "targets": {
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/tools/cert-generator",
        "main": "tools/cert-generator/src/index.ts",
        "tsConfig": "tools/cert-generator/tsconfig.lib.json",
        "outputFileName": "index.cjs",
        "platform": "node",
        "format": ["cjs"],
        "bundle": true,
        "assets": []
      }
    },
    "help": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/index.cjs --help"
      }
    },
    "chain": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/index.cjs chain"
      }
    },
    "root": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/index.cjs root"
      }
    },
    "intermediate": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/index.cjs intermediate"
      }
    },
    "leaf": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/index.cjs leaf"
      }
    }
  }
}
