{
  "name": "cert-generator",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/cert-generator/src",
  "projectType": "library",
  "tags": ["type:tool", "runtime:node"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/cert-generator",
        "main": "tools/cert-generator/src/index.ts",
        "tsConfig": "tools/cert-generator/tsconfig.lib.json",
        "assets": ["tools/cert-generator/*.md"]
      }
    },
    "help": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/src/index.js --help"
      }
    },
    "chain": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/src/index.js chain"
      }
    },
    "root": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/src/index.js root"
      }
    },
    "intermediate": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/src/index.js intermediate"
      }
    },
    "leaf": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "command": "node dist/tools/cert-generator/src/index.js leaf"
      }
    }
  }
}
